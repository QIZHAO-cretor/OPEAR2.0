<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Lorchestre"
    android:background="@drawable/fovdl_appli_orchestre">

    <TextView

        android:id="@+id/textViewOrchestre"
        android:layout_width="235dp"

        android:layout_height="57dp"
        android:gravity="center_horizontal"
        android:text="l'orchestre"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="35dp"

        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backtohomepage"
        app:layout_constraintVertical_bias="0.158" />

    <TextView
        android:id="@+id/cordes"
        android:layout_width="103dp"
        android:layout_height="113dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="cordes"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewOrchestre"
        app:layout_constraintVertical_bias="0.32" />

    <TextView
        android:id="@+id/ventbois"
        android:layout_width="112dp"
        android:layout_height="93dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="vent bois"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cordes"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/cuivres"
        android:layout_width="108dp"
        android:layout_height="97dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="cuivres"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.698"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ventbois"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/claviers"
        android:layout_width="81dp"
        android:layout_height="113dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="claviers"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toEndOf="@+id/cordes"
        app:layout_constraintTop_toTopOf="@+id/cordes"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/percussions"
        android:layout_width="245dp"
        android:layout_height="43dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="percussions"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewOrchestre"
        app:layout_constraintVertical_bias="0.869" />

    <ImageView
        android:id="@+id/backtohomepage"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/fovdl_appli_boutonretour" />


</androidx.constraintlayout.widget.ConstraintLayout>